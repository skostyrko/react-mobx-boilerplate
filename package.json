{
  "name": "react-mobx-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "yarn run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/prod.babel.js --color -p --progress",
    "build:clean": "yarn run test:clean && rimraf ./build",
    "start": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --config ./webpack/dev.babel.js",
    "start:production": "yarn run test && yarn run build && yarn run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean:all": "yarn run test:clean && yarn run build:clean",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:fix": "yarn run lint:js-fix",
    "lint:js": "standard",
    "lint:js-fix": "standard --fix",
    "lint:css": "stylelint \"app/**/*.scss\" --syntax scss",
    "lint:staged": "lint-staged",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "precommit": "yarn run lint:staged",
    "prepush": "yarn test"
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "react-stockcharts": "^0.6.1",
    "react-virtualized": "^9.4.0"
  },
  "devDependencies": {
    "@kadira/storybook-addon-notes": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babili-webpack-plugin": "^0.0.11",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "enzyme": "^2.8.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "ip": "^1.1.5",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "mobx-react-devtools": "^4.2.11",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "postcss-smart-import": "^0.6.10",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "standard": "^9.0.2",
    "style-loader": "^0.16.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx}",
      "!**/app/**/*.stories.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    }
  },
  "babel": {
    "presets": [
      "react",
      "flow",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ],
    "env": {
      "production": {
        "only": [
          "app",
          "webpack"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [],
    "envs": [
      "browser",
      "es6",
      "jest"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "*.{js,jsx}": "standard",
    "*.scss": "stylelint --syntax=scss"
  }
}
