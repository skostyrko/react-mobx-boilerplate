[ignore]
.*/node_modules/stylelint/.*
^config$
.*test.js

[include]
.*/app/.*

[libs]
./node_modules/mobx/lib/mobx.js.flow
./flow/global.js

[options]
esproposal.decorators=ignore
unsafe.enable_getters_and_setters=true
module.name_mapper='.*\(.scss\)' -> 'CSSModule'
module.system=haste
strip_root=true
